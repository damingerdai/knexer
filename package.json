{
  "name": "knexer",
  "version": "0.0.1",
  "description": "a orm tools based on knexjs",
  "main": "index.js",
  "scripts": {
		"init": "sh init.sh",
    "build": "tsc",
    "gulp": "gulp",
    "test": "jest",
    "preinstall": "node tools/check-yarn.js",
    "lint:ts": "eslint src/**/*.ts",
    "seed:make": "./node_modules/.bin/knex --knexfile db/knexfile.js seed:make",
    "seed:run": "./node_modules/.bin/knex --knexfile db/knexfile.js seed:run",
		"db": "docker-compose -f scripts/docker-compose.yml up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damingerdai/knexer.git"
  },
  "keywords": [
    "orm"
  ],
  "author": "明国宾",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/damingerdai/knexer/issues"
  },
  "homepage": "https://github.com/damingerdai/knexer#readme",
  "peerDependencies": {
    "knex": ">=0.20.0"
  },
  "dependencies": {
    "knex": "^0.20.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.4",
    "@types/knex": "^0.16.1",
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "delete": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-no-null": "^1.0.2",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "merge2": "^1.3.0",
    "pg": "^7.18.2",
    "rollup": "^2.0.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
	},
	"resolutions": {
    "**/**/minimist": ">=0.2.1 <1.0.0 || >=1.2.3"
